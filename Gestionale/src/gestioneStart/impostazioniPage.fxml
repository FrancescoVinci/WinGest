               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
   </top>
   <center>
      <TextArea fx:id="testoStampa" editable="false" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets left="20.0" right="20.0" />
         </BorderPane.margin>
      </TextArea>
   </center>
   <bottom>
      <HBox prefHeight="65.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="testoAdd" promptText="Inserire parrocchia">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="buttonInserisci" mnemonicParsing="false" onAction="#addParrocchia" text="Inserisci">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
            </Button>
            <TextField fx:id="testoDelete" promptText="Eliminare parrocchia">
               <HBox.margin>
                  <Insets left="60.0" top="20.0" />
